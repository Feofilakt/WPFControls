        <Button x:Name="RippleTestButton" Margin="10,5,10,5" Click="RippleTestButton_Click" Content="Ripple Test"/>
        <Button x:Name="SplitViewTestButton" Margin="10,5,10,5" Click="SplitViewTestButton_Click" Content="SplitView Test"/>
